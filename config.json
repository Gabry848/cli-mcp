{
  "security": {
    "allowedPartitions": [
      "E:\\"
    ],
    "defaultPath": "E:\\",
    "blockedCommands": [
      "rm",
      "rmdir",
      "del",
      "format",
      "fdisk",
      "diskpart",
      "shutdown",
      "reboot",
      "restart",
      "halt",
      "poweroff",
      "net user",
      "net localgroup",
      "adduser",
      "deluser",
      "passwd",
      "chpasswd",
      "sudo",
      "su",
      "runas",
      "reg delete",
      "reg add",
      "regedit",
      "gpedit",
      "sc delete",
      "sc create",
      "sc config",
      "taskkill",
      "wmic",
      "powercfg",
      "bcdedit"
    ],
    "allowedCommandPatterns": [],
    "maxCommandLength": 500,
    "timeoutSeconds": 30,
    "allowNetworkCommands": false
  }
}
